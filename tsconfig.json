{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./",
    "paths": {
      "@styles/*": [
        "styles/*"
      ],
      "@icons/*": [
        "public/icons/*"
      ],
      "@images/*": [
        "public/images/*"
      ],
      "@fonts/*": [
        "public/fonts/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@tests/*": [
        "tests/*"
      ],
      "@store/*": [
        "store/*"
      ],
      "@models/*": [
        "app/models/*"
      ],
      "@services/*": [
        "services/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": false
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "@styles/*.module.scss"
  ],
  "exclude": [
    "node_modules"
  ]
}
